<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Session01Servlet</servlet-name>
        <servlet-class>com.dyp.fruit.servlet.session.Session01Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Session01Servlet</servlet-name>
        <url-pattern>/session01</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Session02Servlet</servlet-name>
        <servlet-class>com.dyp.fruit.servlet.session.Session02Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Session02Servlet</servlet-name>
        <url-pattern>/session02</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddServlet</servlet-name>
        <servlet-class>com.dyp.fruit.servlet.AddServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddServlet</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
</web-app>